    "script-ext-html-webpack-plugin": "^2.1.4",
    "source-map-loader": "^0.2.4",
    "terser": "^4.6.11",
    "terser-webpack-plugin": "^2.3.5",
    "three": "^0.114.0",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
